programa turtle

	/*Drawing with turtle and frida */
	
	lienzo{
		variable pincel frida =  nuevo pincel("#000000");
		variable pincel diego =  nuevo pincel("#000000");

		variable entero x = 1;

		mientras(x < 400){
			diego.pinta(x);
			frida.pinta(x);
			frida.gira(91);
			diego.gira(-91);
			x = x + 1;
		}

	}
