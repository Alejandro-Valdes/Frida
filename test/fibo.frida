programa fibo
	rutina entero : fibo_iteration(entero: lim){
		variable entero x1 = 0, x2 = 1, temp;
		variable entero cont = 1;
		variable entero res = 0;

		si(lim == 0){		
			res = x1;
		}
		sino_pero(lim == 1){
			res = x2;
		}
		sino{
			mientras(cont < lim){

				res = x1 + x2;

				temp = x2;
				x2 = res;
				x1 = temp;

				cont = cont + 1;
			}
		}

		regresa res;
	}

	rutina entero : fibo_rec(entero: x){
		variable entero x1, x2;

		si(x == 0){
			regresa 0;
		}
		si(x == 1){
			regresa 1;
		}

		x1 = fibo_rec(x - 1);
		x2 = fibo_rec(x - 2);

    	regresa x1 + x2;

	}

	rutina bool : iguales(entero: x1, entero: x2){
		regresa x1 == x2;
	}

	rutina cadena : felicitaciones(bool: res){

		si(res){
			regresa 'felicidades';
		}
		sino{
			regresa 'lo siento';
		}
	}

