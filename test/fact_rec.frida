programa factorial

	/*Algorithm for finding the k-th even natural number */

	rutina entero : fact(entero: x){
		variable entero res = 0;
		si (x == 1){
        	regresa 1;
		}

	    sino{
	        regresa x * fact(x - 1);
	    }

	}
	

	lienzo{
		variable entero res = 0, x = 5, res2 = x;

		res = fact(5);

		imprimir(res);

		mientras (x > 1){
			res2 = res2 * (x - 1);
			x = x - 1;
		}
		imprimir(res2);
	}
