	}

	rutina entero : fibo(entero: x){
		variable entero x1, x2;

		si(x == 0){
			regresa 0;
		}
		si(x == 1){
			regresa 1;
		}

		x1 = fibo(x - 1);
		x2 = fibo(x - 2);

    	regresa x1 + x2;

	}

